                  ",", 
                 RowBox[{"maxparlevel", "=", 
                  RowBox[{"Max", "[", 
                   RowBox[{"maxparlevel", ",", 
                    RowBox[{"++", "parlevel"}]}], "]"}]}], ",", 
                 RowBox[{"parlevel", "--"}]}], "]"}], ";", 
               "\[IndentingNewLine]", 
               RowBox[{"j", "++"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
            RowBox[{"texstring", "=", 
             RowBox[{"StringJoin", "[", 
              RowBox[{"texstring", ",", 
               RowBox[{"OpenParenthesis", "[", 
                RowBox[{
                 RowBox[{"maxparlevel", "-", "1"}], ",", "currentparchar"}], 
                "]"}]}], "]"}]}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"i", "\[LessEqual]", " ", 
             RowBox[{"Length", "@", "parchars"}]}], "&&", 
            RowBox[{
             RowBox[{"parchars", "[", 
              RowBox[{"[", "i", "]"}], "]"}], "===", "$TexCloseParChar"}]}], 
           ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"j", "=", 
             RowBox[{"i", "-", "1"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"parlevel", "=", "1"}], ";", "\[IndentingNewLine]", 
