         "\[IndentingNewLine]", 
         RowBox[{"Print", "[", "\"\<Typesetting OK.\>\"", "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{"openfileq", ",", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{"$TexViewInternal", ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"Print", "[", 
               RowBox[{
               "\"\<Importing file: \>\"", ",", " ", "file", ",", 
                "$TexViewExt"}], "]"}], ";", "\[IndentingNewLine]", 
              RowBox[{"Module", "[", 
